{
    "name": "sap-hdbext-promisfied",
    "version": "2.202203.3",
    "description": "Promise wrapper for @sap/hdbext",
    "main": "./index.cjs",
    "exports": {
        "./package.json": "./package.json",
        ".": [
            {
                "import": "./index.js",
                "require": "./index.cjs"
            },
            "./index.cjs"
        ]
    },
    "type": "module",
    "module": "./index.js",
    "types": "./@types/index.d.ts",
    "keywords": [
        "promise",
        "hdbext",
        "sap",
        "hana",
        "database"
    ],
    "author": "jung-thomas",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP-samples/hana-hdbext-promisfied-example"
    },
    "dependencies": {
        "@sap/hdbext": "^7.6.3",
        "@sap/xsenv": "^3.2.1",
        "debug": "^4.3.4",
        "dotenv": "^16.0.0"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.23",
        "mocha": "^9.2.2"
    },
    "scripts": {
        "start": "node test",
        "types": "tsc  --declaration --allowJs --emitDeclarationOnly --outDir @types",
        "prodinstall": "npm install --only=prod",
        "test": "mocha './tests/*.Test.js' --parallel --timeout 5000"
    },
    "engines": {
        "node": "^12.18.0 || ^14.0.0 || ^16.0.0"
    }
}
